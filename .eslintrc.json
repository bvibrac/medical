{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "react/destructuring-assignment": 0,
    "react/prop-types": 0,
    "react/jsx-no-bind": 0,
    "react/no-access-state-in-setstate": 0,
    "no-nested-ternary": 0,
    "react/no-unused-state" : 0
  }
}
